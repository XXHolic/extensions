{
  "name": "Capture Request",
  "description": "Capture Request",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "scripting",
    "tabs",
    "storage",
    "activeTab",
    "webRequest",
    "downloads"
  ],
  "host_permissions": ["<all_urls>"],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "/images/icon_300.png",
      "32": "/images/icon_300.png",
      "48": "/images/icon_300.png",
      "128": "/images/icon_300.png"
    }
  },
  "icons": {
      "16": "/images/icon_300.png",
      "32": "/images/icon_300.png",
      "48": "/images/icon_300.png",
      "128": "/images/icon_300.png"
  }
}
